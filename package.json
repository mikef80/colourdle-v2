{
  "name": "colourdle-v2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev-host": "react-router dev --host",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "seed-dev": "dotenv -e .env.dev -- prisma db seed",
    "seed-test": "dotenv -e .env.test -- prisma db seed",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "migrate-dev": "dotenv -e .env.dev -- prisma migrate dev",
    "migrate-test": "dotenv -e .env.test -- prisma migrate dev",
    "migrate-reset": "dotenv -e .env.dev -- prisma migrate reset"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/run-seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "@remix-run/node": "^2.15.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.8",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.5",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prisma": "^6.8.2",
    "react-router-devtools": "^1.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
}